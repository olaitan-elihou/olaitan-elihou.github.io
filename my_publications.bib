@book{amadou_sanni_bilan_2019,
	address = {Sarrebuck, Allemagne},
	title = {Bilan et {Profils} des abandons scolaires au primaire au {Bénin}: {Les} évidences d’une enquête en milieu scolaire en 2016 chez les 12-17 ans dans l'arrondissement de {Tchaourou}},
	copyright = {All rights reserved},
	isbn = {978-613-8-45400-7},
	shorttitle = {Bilan et {Profils} des abandons scolaires au primaire au {Bénin}},
	abstract = {Si les chiffres indiquent que la scolarisation des enfants est à la hausse en Afrique Subsaharienne, l’achèvement des études primaires reste un défi de développement à ne pas négliger. Après l’échéance du Programme Décennal de Développement du Secteur de l’Éducation (PDDSE : 2006-2015) visant l’achèvement systématique du primaire en 2015, ce travail s’intéresse au niveau et aux profils des abandons scolaires au primaire au sein des 12-17 ans révolus dans l’arrondissement de Tchaourou, au nord-Bénin. À partir des données d’une enquête sociodémographique réalisée en 2016 dans cette localité, il est établi que plus d’un (01) adolescent sur dix (10) sort du primaire sans l’achever avec succès. Les jeunes filles, les enfants des foyers pauvres ou ceux ne résidant pas dans les foyers familiaux de leurs parents biologiques, les enfants tardivement scolarisés ou en retard de scolarisation (redoublements répétitifs) sont, particulièrement, touchés par le phénomène. Par ailleurs, plus le niveau d’instruction du chef de foyer est élevé ou que la localité de résidence est urbanisée, moins ces abandons du système éducatif primaire sont susceptibles de se produire.},
	language = {French},
	publisher = {Éditions universitaires européennes},
	author = {Amadou Sanni, Mouftaou and Adjé, Elihou},
	month = jan,
	year = {2019},
}

